{
  "name": "micro-klarspraak",
  "description": "Check your text against lix and kansellisten",
  "version": "1.1.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "start": "micro",
    "dev": "micro-dev",
    "deploy": "npm test && now && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "klarspråk",
    "klarspraak",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/micro-klarspraak.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-klarspraak/issues"
  },
  "homepage": "https://github.com/telemark/micro-klarspraak#readme",
  "devDependencies": {
    "ava": "1.4.1",
    "coveralls": "3.0.3",
    "micro-dev": "3.0.0",
    "nyc": "14.1.0",
    "standard": "12.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "check-kansellisten": "1.0.0",
    "lix-index": "1.0.1",
    "micro": "9.3.4",
    "striptags": "3.1.1"
  }
